# Sample environment variables
# Copy this file to .env and fill in your actual values

# Hunter API key for account discovery 
HUNTER_API_KEY=your_hunter_api_key_here

# Flask secret key (use a strong random string)
SECRET_KEY=generate_a_strong_secret_key_here

# Database URI (default is SQLite)
DATABASE_URI=sqlite:///passwordservice.db

# Optional API keys for additional services
HAVEIBEENPWNED_API_KEY=your_hibp_api_key_here
CLEARBIT_API_KEY=your_clearbit_api_key_here
FULLCONTACT_API_KEY=your_fullcontact_api_key_here

# Feature flags
HOLEHE_ENABLED=false